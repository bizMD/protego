#break {
	@include size(100% 20%);
}

#body {
	@include outer-container;
	@include size(100%);

	#header {
		@include span-columns(6);
		@include shift(1);
	}

	#data-case {
		@include span-columns(6);
		@include shift(3);

		#data {
			@include padding(0);
			@include margin(0);
		}

		#tickets {
			@include position(relative, 0 0 0 0);
			margin: 0 auto;
		}
	}
}